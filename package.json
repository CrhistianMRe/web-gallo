{
  "devDependencies": {
    "concurrently": "^9.2.1",
    "electron": "^33.4.11"
  },
  "scripts": {
    "dev:server": "node server/server.js",
    "dev:web": "cd web && npm run dev",
    "dev:electron": "NODE_ENV=development electron electron/main.js",
    "dev:desktop": "concurrently \"npm run dev:server\" \"npm run dev:web\" \"npm run dev:electron\"",
    "build:web": "cd web && npm run build",
    "start:electron": "NODE_ENV=production electron electron/main.js"
  }
}
